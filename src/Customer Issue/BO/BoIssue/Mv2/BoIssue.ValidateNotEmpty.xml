<BusinessLogic methodName="validateNotEmpty" businessObjectClass="BoIssue" asynchronous="false" accessibility="PUBLIC" final="false" module="CORE">
  <Parameters>
    <MethodInput name="messageCollector" type="messageCollector" />
  </Parameters>
  <Code language="JavaScript"><![CDATA[
    
        var newError;
	if (Utils.isEmptyString(this.getText()))
	{
		newError = {"level": "error", 
                    "objectClass": "BoIssue", 
                    "messageID": "CasBoIssueDescriptionEmpty"};
        messageCollector.add(newError);
	}
		
   ]]></Code>
</BusinessLogic>