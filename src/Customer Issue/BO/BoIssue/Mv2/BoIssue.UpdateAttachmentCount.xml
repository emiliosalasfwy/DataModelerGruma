<BusinessLogic methodName="updateAttachmentCount" businessObjectClass="BoIssue" businessObjectType="" asynchronous="false" accessibility="PUBLIC" final="false" module="CORE" simpleEditorOnly="">
  <Parameters>
    <MethodInput name="update" type="String" />
  </Parameters>
<Code language="JavaScript">
<![CDATA[if (update === "Add") 
{
  this.getLuIssueAttachmentCount().set('issueAttachmentCount', (this.getLuIssueAttachmentCount().getIssueAttachmentCount() + 1));
}
else if (update === "Delete") 
{
  this.getLuIssueAttachmentCount().set('issueAttachmentCount', (this.getLuIssueAttachmentCount().getIssueAttachmentCount() - 1));
}]]>
</Code>
  <Return name="" value="" />
</BusinessLogic>